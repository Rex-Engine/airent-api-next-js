// airent imports
import { dispatchWith } from '@airent/api';
import { handleWith } from '<%- apiNextPackage %>';

// config imports
import { dispatcherConfig } from '<%- dispatcherConfigPackage %>';
import { handlerConfig } from '<%- handlerConfigPackage %>';

// function imports
import { parser, authorizer, executor } from '<%- entry.parsed.sourcePackage %>';

const dispatcher = dispatchWith({
  ...dispatcherConfig,
  authorizer,
  parser,
  executor,
});

export const POST = handleWith(dispatcher, handlerConfig);
