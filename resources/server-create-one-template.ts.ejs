<% if (entity.api && entity.api.booleans.hasCreateOne) { -%>
import * as <%= entity.api.strings.actionClass %> from '@/backend/entities/generated/<%= utils.toKababCase(entity.name) %>-action';
import { handleCreateOne } from 'airent-api';
import { <%= entity.api.strings.createOneBody %> } from '<%= entity.api.request.import %>';
<%- config.authenticatorImport %>

export const POST = handleCreateOne({
  bodyZod: <%= entity.api.strings.createOneBody %>,
  apiExecutor: <%= entity.api.strings.actionClass %>.<%= entity.api.strings.createOneAction %>,
  authenticator,
  options: {
  <%_ if ((entity.api.internalMethods ?? []).includes('createOne')) { -%>
    requireInternal: true,
  <%_ } -%>
  <%_ if ((entity.api.adminMethods ?? []).includes('createOne')) { -%>
    requireAdmin: true,
  <%_ } -%>
  <%_ if ((entity.api.publicMethods ?? []).includes('createOne')) { -%>
    requireLogin: false,
  <%_ } -%>
  },
});
<% } -%>
